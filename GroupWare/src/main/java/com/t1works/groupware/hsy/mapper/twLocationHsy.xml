<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="twLocationHsy">

	<!-- 회사위치테이블에 있는 정보 가져오기   -->
	<select id="twLocationAjax" resultType="com.t1works.groupware.hsy.model.TwLocationHsyVO">
